package  {		import flash.display.MovieClip;			public class PopUp extends MovieClip 	{		private var _myCategory: String;		private var _myInfo:XML;				public function PopUp(category:String, info:XML) 		{			_myCategory = category;			_myInfo = info;			this.category.text = _myCategory;			this.title.text = _myInfo.title;						this.info.htmlText = "Multi-Location Control: " + _myInfo.popup.multiLocationControl + "<br /><br />Wallplates Type: " 			+ _myInfo.popup.wallplatesType + "<br /><br />Colors: " + _myInfo.popup.deviceColors + "<br /><br />Neutral Required: " + _myInfo.popup.neutralRequired			 + "<br /><br />Color Kits: " + _myInfo.popup.colorKits + "<br /><br />Kit Colors: " + _myInfo.popup.kitColors;		}	}	}